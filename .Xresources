*Rxvt*scrollBar:		false
*Rxvt*imLocale:			en_US.UTF-8
*Rxvt*loginShell:		false
*Rxvt*saveLines: 		6000
*Rxvt*inheritPixmap:		false
*Rxvt*xftAntialias:		false
*Rxvt*jumpScroll:		false
*Rxvt*tintColor:		white
*Rxvt*perl-ext-common:		default,tabbed,url-select,clipboard
*Rxvt*url-launcher:		google-chrome
*Rxvt*matcher.button:		1
URxvt.clipboard.autocopy:	true
URxvt.iso14755:			false
*fading:						20

!	url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: google-chrome
URxvt.url-select.underline: true

!	Clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste

!URxvt*font: 			xft:tewi2a:pixelsize=11
!URxvt*boldFont: 		xft:tewi2a:bold:pixelsize=11

!URxvt*font:			xft:Terminus:12
!URxvt*boldFont:			xft:Terminus:12

URxvt*font:			xft:Monaco:11
URxvt*boldFont:			xft:Monaco:11
!*Rxvt*letterSpace:		-1

!URxvt*colorUL: 		#e74c3c
*Rxvt*iconFile:			/home/roelof/term.svg

URxvt.urgentOnBell: 		True
URxvt*tabbed.tabbar-fg: 	1
URxvt*tabbed.tabbed-bg: 	0
URxvt*tabbed.tab-fg:		7
URxvt*tabbed.tab-bg:		0


